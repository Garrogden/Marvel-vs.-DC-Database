drop database Marvel_DC ;
create database Marvel_DC2;
use Marvel_DC2;
drop table Powers;
create table Powers
(Power_ID varchar (30) PRIMARY KEY,
Power_Name varchar (100)
);
drop table Marvel_Characters;
create table Marvel_Characters
(Character_Name varchar (20) PRIMARY KEY ,
Character_Type varchar (15),
Power_ID varchar (30),
Gender varchar(255),
Attributes varchar(255),
Motive varchar(255),
Weakness varchar(255)
);
drop table DC_Characters;
create table DC_Characters
(Character_Name varchar (20) PRIMARY KEY,
Character_Type varchar (15),
Power_ID varchar (30),
Gender varchar(255),
Attributes varchar(255),
Motive varchar(255),
Weakness varchar(255)
);
drop table Hero_Powers;
create table Hero_Powers
(Name varchar (200) PRIMARY KEY,
Power_ID varchar (30));
drop table Villain_Powers;
create table Villain_Powers
(Name varchar (200) PRIMARY KEY,
Power_ID varchar (30));
